<section>
    <h3 id="inputs">{{#if title}}{{title}}{{else}}Indexable{{/if}}</h3>
    {{#each indexables}}
        <table class="table table-sm table-bordered">
            <tbody>
                <tr>
                    <td class="col-md-4">
                        <code>{{indexableSignature this}}:{{> link-type type=returnType }}</code>
                    </td>
                </tr>
                {{#if line}}
                    {{#unless ../disableSourceCode}}
                        <tr>
                            <td class="col-md-4">
                                <div class="io-line">Defined in <a href="" data-line="{{{line}}}" class="link-to-prism">{{../file}}:{{{line}}}</a></div>
                            </td>
                        </tr>
                    {{/unless}}
                {{/if}}
                {{#if description}}
                <tr>
                    <td class="col-md-4">
                        <div class="io-description">{{{parseDescription description ../depth}}}</div>
                    </td>
                </tr>
                {{/if}}
            </tbody>
        </table>
    {{/each}}
</section>
