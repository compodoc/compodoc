<section>
    <h3 id="methods">{{#if title}}{{title}}{{else}}Methods{{/if}}</h3>
    {{#each methods}}
        <table class="table table-sm table-bordered">
            <tbody>
                <tr>
                    <td class="col-md-4">
                        <b>{{#if modifierKind}}<span class="modifier">{{modifKind modifierKind}}</span>{{/if}} {{name}}</b>
                    </td>
                </tr>

                <tr>
                    <td class="col-md-4">
                        {{#if modifierKind}}
                            <span class="modifier-icon method fa fa-play {{modifIcon modifierKind}}">
                                <span class="fa fa-{{modifIcon modifierKind}}"></span>
                            </span>
                        {{/if}}<code>{{{functionSignature this}}}</code>
                    </td>
                </tr>

                {{#or description jsdoctags returnType}}
                <tr>
                    <td class="col-md-4">
                        {{#if description}}
                            <div class="io-description">{{{parseDescription description ../depth}}}</div>
                        {{/if}}
                        {{#if jsdoctags}}
                            <div class="io-description">
                                {{#jsdoc-params jsdoctags}}
                                    <b>Parameters :</b>
                                    <ul class="jsdoc-params">
                                        {{#each tags}}
                                            <li>
                                                <div>
                                                    <b>{{name}}</b>
                                                    {{#if type}}
                                                        : {{> link-type type=type }}
                                                    {{/if}}
                                                </div>
                                                <div>
                                                    {{{comment}}}
                                                </div>
                                            </li>
                                        {{/each}}
                                    </ul>
                                {{/jsdoc-params}}
                                {{#jsdoc-example jsdoctags}}
                                    <b>Example :</b>
                                    {{#each tags}}
                                        <div>
                                            {{{comment}}}
                                        </div>
                                    {{/each}}
                                {{/jsdoc-example}}
                            </div>
                        {{/if}}
                        {{#if returnType}}
                            <div class="io-description">
                                <b>Returns: </b>{{> link-type type=returnType }}
                            </div>
                            {{#if jsdoctags}}
                                <div class="io-description">
                                    {{{jsdoc-returns-comment jsdoctags}}}
                                </div>
                            {{/if}}
                        {{/if}}
                    </td>
                </tr>
                {{/or}}
            </tbody>
        </table>
    {{/each}}
</section>
