<section>
    {{#each enums}}
        <table class="table table-sm table-bordered">
            <tbody>
                <tr>
                    <td class="col-md-4">
                        <b>{{name}}</b>
                    </td>
                </tr>
                {{#if description}}
                    <tr>
                        <td class="col-md-4">
                            <div class="io-description">{{{parseDescription description ../depth}}}</div>
                        </td>
                    </tr>
                {{/if}}
                {{#each childs}}
                    {{#if name}}
                        <tr>
                            <td class="col-md-4">
                                <b>{{name}}</b>
                            </td>
                        </tr>
                    {{/if}}
                    {{#if value}}
                        <tr>
                            <td class="col-md-4">
                                <i>Value: </i><code>{{value}}</code>
                            </td>
                        </tr>
                    {{/if}}
                {{/each}}
            </tbody>
        </table>
    {{/each}}
</section>
