<h3>Classes</h3>
<div class="container-fluid classes">
    <div class="row">
        {{#each classes}}
        <div class="col-sm-3">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title">{{name}}</h4>
                </div>
                <div class="card-block">
                    <footer class="text-center">
                        <a href="./classes/{{name}}.html" class="btn btn-default">Browse</a>
                    </footer>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>
